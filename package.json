{
  "name": "aavegotchi-pet-sitter",
  "version": "2.0.0",
  "description": "Automated Aavegotchi pet sitter bot for Base chain",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && npm run copy:abi",
    "copy:abi": "mkdir -p dist/abi && cp src/abi/*.json dist/abi/",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "watch": "tsx watch src/index.ts",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "test": "tsx src/test/test-dynamic-gas.ts",
    "test:interact": "tsx src/test/force-interact.ts",
    "test:batch": "tsx src/test/batch-interact.ts"
  },
  "keywords": ["aavegotchi", "base", "blockchain", "bot", "pet"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "viem": "^2.7.0",
    "dotenv": "^16.4.0",
    "winston": "^3.11.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}